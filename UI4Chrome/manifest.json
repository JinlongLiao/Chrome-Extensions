{
   "name": "Chrome仪表盘",
  "description": "Chrome仪表盘，优化Chrome的首页或新标签",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "img/64.png"
  },
  "icons": {
    "32": "img/32.png",
    "64": "img/64.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "unlimitedStorage",
    "history",
    "topSites",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "notifications"
  ],
  "version": "0.1"
}